
@{
    ViewBag.Title = "VerifyAccount";
}

<h2>VerifyAccount</h2>

@if (ViewBag.Status)
{
    <div class="alert alert-success">

        <strong> Congratulation @ViewBag.name Your link was Activited Successfully! </strong> @Html.ActionLink("Login Here", "login")

    </div>
}
else
{
    <div class="alert alert-danger">

        <strong> Something Went Wrong!! </strong> @ViewBag.Message

    </div>
}

